 javascript:(function(){function randomizeNumber(num){var magnitude=Math.pow(10,Math.floor(Math.log10(Math.abs(num)))+1);return Math.floor(Math.random()*magnitude);}function processNode(node){if(node.nodeType===3){var text=node.nodeValue;var newText=text.replace(/\b\d+(\.\d+)?\b/g,function(match){var num=parseFloat(match);return randomizeNumber(num);});if(text!==newText){node.nodeValue=newText;}}else if(node.nodeType===1&&node.nodeName!=='SCRIPT'&&node.nodeName!=='STYLE'){for(var i=0;i<node.childNodes.length;i++){processNode(node.childNodes[i]);}}}processNode(document.body);alert('Numbers on the page have been randomized!');})();  
