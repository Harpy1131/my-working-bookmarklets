javascript:(function(){var memeUrls=["https://i.imgflip.com/3p6xhz.jpg","https://i.imgflip.com/30b1gx.jpg","https://i.imgflip.com/1bij.jpg"];function randomMeme(){return memeUrls[Math.floor(Math.random()*memeUrls.length)];}function walk(node){if(node.nodeType===3){if(/\d/.test(node.nodeValue)){var newHTML=node.nodeValue.replace(/\d+/g,function(match){var url=randomMeme();return '<img src=\"'+url+'\" style=\"width:50px;height:auto;vertical-align:middle\">';});var span=document.createElement("span");span.innerHTML=newHTML;node.parentNode.replaceChild(span,node);}}else if(node.nodeType===1 && node.nodeName!=="SCRIPT" && node.nodeName!=="STYLE"){for(var i=0;i<node.childNodes.length;i++){walk(node.childNodes[i]);}}}walk(document.body);})();
