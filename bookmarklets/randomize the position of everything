javascript:(function(){function getRandomPosition(){return Math.floor(Math.random()*100)+'%';}function randomizeElementPosition(element){if(element.tagName!=='HTML'&&element.tagName!=='BODY'){element.style.position='relative';element.style.left=getRandomPosition();element.style.top=getRandomPosition();if(element.tagName==='IMG'){element.style.transform='rotate('+Math.floor(Math.random()*360)+'deg)';}}else{element.style.position='static';}}function randomizeAllPositions(){const allElements=document.querySelectorAll('*');for(let i=0;i<allElements.length;i++){randomizeElementPosition(allElements[i]);}}randomizeAllPositions();})();
